                                                    <ul>
                                                        <li class="text-danger"><span asp-validation-for="Vehicle.VehicleModelId" class="text-danger"></span></li>
                                                        <li class="text-danger"><span asp-validation-for="Vehicle.RegistrationPlateNumber" class="text-danger"></span></li>
                                                        <li class="text-danger"><span asp-validation-for="Vehicle.RegistrationNumber" class="text-danger"></span></li>
                                                        <li class="text-danger"><span asp-validation-for="Vehicle.Photos" class="text-danger"></span></li>
                                                        <li class="text-danger"> <span asp-validation-for="Vehicle.CupunCode" class="text-danger"></span></li>
                                                    </ul>
                                                </div>
                                                <div class="col-12 col-md-6">
                                                    <div class="form_item">
                                                        <label>Enter Make </label><span>*</span>
                                                        <select asp-items="@(new SelectList(ViewBag.make,"Id","Name"))" id="SelectMake">
                                                            <option value="0">Select a Make</option>
                                                        </select>
                                                    </div>
                                                    <div class="form_item">
                                                        <label>Vehicle Manufacturer Year </label><span>*</span>
                                                        <input asp-for="Vehicle.VehicleManufacturerYear" />
                                                        <span asp-validation-for="Vehicle.VehicleManufacturerYear" class="text-danger"></span>
                                                    </div>
                                                    <div class="form_item">
                                                        <label>No. of Seats  </label><span>*</span>
                                                        <input asp-for="Vehicle.NoOfSeats" />
                                                        <span asp-validation-for="Vehicle.NoOfSeats" class="text-danger"></span>
                                                    </div>
                                                    <div class="form_item">
                                                        <label>Vehicle Transmission Type  </label><span>*</span>
                                                        <select asp-for="Vehicle.TransmissionTypeId">
                                                            @*<option value="0" selected>Select</option>*@

                                                            @foreach (var item in ViewBag.transmissionTypes)
                                                            {
                                                                <option value="@item.Id">@item.Name</option>
                                                            }
                                                        </select>
                                                    </div>
                                                    <div class="form_item">
                                                        <label>No. of Cylinders </label><span>*</span>
                                                        <input asp-for="Vehicle.NoOfCylinders" />
                                                        <span asp-validation-for="Vehicle.NoOfCylinders" class="text-danger"></span>
                                                    </div>
                                                    <div class="form_item">
                                                        <label>Select Fuel Type </label><span>*</span>
                                                        <select asp-for="Vehicle.FuelTypeId">
                                                            @*<option value="0" selected>Select</option>*@

                                                            @foreach (var item in ViewBag.fuelTypes)
                                                            {
                                                                <option value="@item.Id">@item.Name</option>
                                                            }
                                                        </select>
                                                    </div>
                                                    <div class="form_item">
                                                        <label>City Fuel Economy (L/100km) </label><span>*</span>
                                                        <input asp-for="Vehicle.CityFuelEconomy" />
                                                        <span asp-validation-for="Vehicle.CityFuelEconomy" class="text-danger"></span>
                                                    </div>
                                                </div>
                                                <div class="col-12 col-md-6">
                                                    <div class="form_item">
                                                        <label>Enter Model </label><span>*</span>
                                                        <select asp-for="Vehicle.VehicleModelId" asp-items="@(new SelectList(string.Empty,"Id","Name"))" id="SelectModel">
                                                            <option value="0">Select a Model</option>
                                                        </select>
                                                        <span asp-validation-for="Vehicle.VehicleModelId" class="text-danger"></span>
                                                    </div>
                                                    <div class="form_item">
                                                        <label>Select Body Type  </label><span>*</span>
                                                        <select asp-for="Vehicle.BodyTypeId">
                                                            @*<option value="0" selected>Select</option>*@

                                                            @foreach (var item in ViewBag.bodyType)
                                                            {
                                                                <option value="@item.Id">@item.Name</option>
                                                            }
                                                        </select>
                                                    </div>
                                                    <div class="form_item">
                                                        <label>No. of Doors </label><span>*</span>
                                                        <input asp-for="Vehicle.NoOfDoors" />
                                                        <span asp-validation-for="Vehicle.NoOfDoors" class="text-danger"></span>
                                                    </div>
                                                    <div class="form_item">
                                                        <label>Select Drive Type </label><span>*</span>
                                                        <select asp-for="Vehicle.DriveTypeId">
                                                            @*<option value="0" selected>Select</option>*@

                                                            @foreach (var item in ViewBag.driveTypes)
                                                            {
                                                                <option value="@item.Id">@item.Name</option>
                                                            }
                                                        </select>
                                                    </div>
                                                    <div class="form_item">
                                                        <label>Enter Engine Capacity (sm<sup>3</sup>)</label><span>*</span>
                                                        <input asp-for="Vehicle.EngineCapacity" />
                                                        <span asp-validation-for="Vehicle.EngineCapacity" class="text-danger"></span>
                                                    </div>
                                                    <div class="form_item">
                                                        <label>Vehicle Status </label><span>*</span>
                                                        <select asp-for="Vehicle.Status">
                                                            <option value="used">Used</option>
                                                            <option value="new">New</option>
                                                        </select>
                                                    </div>
                                                    <div class="form_item">
                                                        <label>Highway Fuel Economy (L/100km) </label><span>*</span>
                                                        <input asp-for="Vehicle.HwyFuelEconomy" />
                                                        <span asp-validation-for="Vehicle.HwyFuelEconomy" class="text-danger"></span>
                                                    </div>
                                                </div>
                                                <div class="d-flex justify-content-end w-100 px-3">
                                                    <div id="stp1nxt2" class="costom_nexn_perv">
                                                        Save & PROCEED to next step
                                                        <span class="fa fa-angle-right"></span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
